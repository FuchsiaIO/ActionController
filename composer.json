{
  "minimum-stability": "dev",
  "name": "FuchsiaIO/ActionController",
  "type": "library",
  "description": "A Controller-View Implementation.",
  "license": "MIT",
  "keywords": ["FuchsiaIO", "ActionController", "controller-view"],
  "authors": [
    {
      "name": "Benjamin J. Anderson",
      "email": "andeb2804@gmail.com"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/arnaud-lb/MtHaml",
      "type": "git"
    },
    {
      "url": "https://github.com/FuchsiaIO/ActionView",
      "type": "git"
    }
  ],
  "require": {
    "mnsami/composer-custom-directory-installer": "1.0.*",
    "mthaml/mthaml": "@dev",
    "FuchsiaIO/ActionView": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-nightly": "master"
    }
  },
  "extra":{
    "installer-paths":{
      "./vendor/action_view/vendor/haml": ["mthaml/mthaml"],
      "./vendor/action_view": ["FuchsiaIO/ActionView"]
    }
  }
}